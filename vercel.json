{
  "builds": [
    {
      "src": "api/create-payment.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/cashfree/create-order.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/cashfree/order-status.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/cashfree/health.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
