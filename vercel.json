{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/cashfree/create-order.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/cashfree/order-status.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/cashfree/health.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
